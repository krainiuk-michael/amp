# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/
/web/themes/
/web/profiles/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.*local.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest values.
/web/sites/simpletest
/files/simpletest
/web/phpunit.xml

# Ignore files generated by PhpStorm
/.idea/

# Various temp files.
*.DS_Store
*.swp
*.swo
.vagrant

# Local Vagrant config file
/Vagrant.local.yml

# Docker files
docker-compose.override.yml

# Ignore git submodules
/drush/**/.git
/vendor/**/.git
/docroot/**/.git
/web/**/.git
